{
	"name": "@craft-brew/queue-ingest",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "./src/index.ts",
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"start": "bun src/index.ts",
		"docker:build": "docker build -t ghcr.io/${npm_config_user:-$USER}/queue-ingest:local -f apps/queue-ingest/Dockerfile .",
		"docker:run": "docker run --rm ghcr.io/${npm_config_user:-$USER}/queue-ingest:local"
	},
	"dependencies": {
		"@craft-brew/database": "workspace:*",
		"@craft-brew/protocol": "workspace:*",
		"@craft-brew/redis": "workspace:*",
		"chalk": "^5.6.2",
		"drizzle-orm": "^0.45.1",
		"mqtt": "^5.10.1"
	},
	"devDependencies": {
		"@types/node": "^22.10.2",
		"typescript": "^5.3.3"
	}
}
